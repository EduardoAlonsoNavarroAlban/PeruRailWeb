<+>package com.everis.base.stepDefinitions;\r\n\r\nimport com.everis.base.questions.ResponseCode;\r\nimport com.everis.base.questions.ResponseUser;\r\nimport com.everis.base.tasks.GetUser;\r\nimport com.everis.base.tasks.PostUser;\r\nimport com.everis.base.tasks.GetUsersPage;\r\nimport io.cucumber.java.Before;\r\nimport io.cucumber.java.en.And;\r\nimport io.cucumber.java.en.Given;\r\nimport io.cucumber.java.en.Then;\r\nimport io.cucumber.java.en.When;\r\nimport net.serenitybdd.screenplay.Actor;\r\nimport net.serenitybdd.screenplay.actors.OnStage;\r\nimport net.serenitybdd.screenplay.actors.OnlineCast;\r\nimport net.serenitybdd.screenplay.rest.abilities.CallAnApi;\r\n\r\nimport static net.serenitybdd.screenplay.GivenWhenThen.seeThat;\r\nimport static net.serenitybdd.screenplay.actors.OnStage.theActorInTheSpotlight;\r\nimport static org.hamcrest.CoreMatchers.equalTo;\r\nimport static org.hamcrest.CoreMatchers.is;\r\n\r\npublic class NetflixSD {\r\n\r\n    @Before\r\n    public void setTheStage() {\r\n        OnStage.setTheStage(new OnlineCast());\r\n    }\r\n\r\n    @Given(\"que {actor} verifica que la aplicacion esta operativa\")\r\n    public void queOmarVerificaQueLaAplicacionEstaOperativa(Actor actor) {\r\n        //TODO: Esto debe ser un ability\r\n        actor.whoCan(CallAnApi.at(\"https://reqres.in/api\"));\r\n    }\r\n\r\n    @When(\"obtiene la lista: {int} de clientes suscritos\")\r\n    public void obtieneLaListaDeClientesSuscritos(int i) {\r\n        theActorInTheSpotlight().attemptsTo(GetUsersPage.fromPage(i));\r\n    }\r\n\r\n    @Then(\"valida que la respuesta es {int}\")\r\n    public void validaQueLaRespuestaEs(int i) {\r\n        theActorInTheSpotlight().should(seeThat(\"El c√≥digo de respuesta\", ResponseCode.getStatus(), equalTo(i)));\r\n    }\r\n\r\n\r\n    @When(\"obtiene al cliente: {int} suscrito\")\r\n    public void obtieneAlClienteSuscrito(int i) {\r\n        theActorInTheSpotlight().attemptsTo(GetUser.fromPage(i));\r\n    }\r\n\r\n    @Then(\"valida que el correo es {string}\")\r\n    public void validaQueElCorreoEs(String correo) {\r\n        theActorInTheSpotlight().should(seeThat(\"El email de respuesta\", ResponseUser.getEmail(), is(correo)));\r\n    }\r\n\r\n    @When(\"obtiene la lista: {int} de clientes suscritos como objeto\")\r\n    public void obtieneLaListaDeClientesSuscritosComoObjeto(int i) {\r\n\r\n    }\r\n\r\n    @Then(\"valida que el nombre es {string}\")\r\n    public void validaQueElNombreEs(String nombre) {\r\n        theActorInTheSpotlight().should(seeThat(\"El nombre de respuesta\", ResponseUser.getFirstName(), is(nombre)));\r\n    }\r\n\r\n    @Then(\"valida que el apellido es {string}\")\r\n    public void validaQueElApellidoEs(String apellido) {\r\n        theActorInTheSpotlight().should(seeThat(\"El apellido de respuesta\", ResponseUser.getLastName(), is(apellido)));\r\n    }\r\n\r\n\r\n    @And(\"inserto los valores del cliente con nombre {string} y trabajo {string}\")\r\n    public void insertoLosValoresDelClienteConNombreYTrabajo(String nombre, String apellido) {\r\n        theActorInTheSpotlight().attemptsTo(PostUser.fromPage(nombre, apellido));\r\n    }\r\n\r\n    @And(\"valida el nombre sea {string}\")\r\n    public void validaElNombreSea(String name) {\r\n        theActorInTheSpotlight().should(seeThat(\"El nombre de respuesta\", ResponseUser.getName(), is(name)));\r\n    }\r\n\r\n    @And(\"valida el trabajo sea {string}\")\r\n    public void validaElTrabajoSea(String job) {\r\n        theActorInTheSpotlight().should(seeThat(\"El trabajo de respuesta\", ResponseUser.getJob(), is(job)));\r\n    }\r\n}\r\n
